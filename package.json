{
  "name": "echarts-composables",
  "version": "0.2.2",
  "author": "ryancui- <cuizhaowei92@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/packages/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "vite --config vite.site.config.ts",
    "dev:docs": "vitepress dev packages",
    "preview": "vite preview --port 4173",
    "test": "vitest --environment jsdom",
    "test:coverage": "vitest run --coverage",
    "build:packages": "vite build",
    "build:docs": "vitepress build packages",
    "build:site": "vite build --config vite.site.config.ts",
    "ci-check": "run-p type-check lint-check test:coverage",
    "type-check": "vue-tsc",
    "lint-check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dist": "./dist/index.mjs",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "peerDependencies": {
    "echarts": "^5.3.3"
  },
  "peerDependenciesMeta": {
    "echarts": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.52",
    "@vicons/carbon": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.4.0",
    "@vueuse/core": "^10.3.0",
    "c8": "^7.12.0",
    "echarts": "^5.4.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "jsdom": "^20.0.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6",
    "unocss": "^0.54.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^1.4.1",
    "vitepress": "1.0.0-alpha.26",
    "vitest": "^0.21.0",
    "vue": "^3.2.37",
    "vue-echarts": "^6.2.3",
    "vue-router": "^4.1.3",
    "vue-tsc": "^1.8.8"
  }
}
